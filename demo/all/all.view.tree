$mol_demo_all $mol_view sub /
	<= medium $mol_demo_medium
		name <= name \$mol_view
		title <= mediumLabel \Fit to content
	<= small $mol_demo_small
		name <= name \$mol_view
		title <= smallLabel @ \Minimum screen
	<= large $mol_demo_large
		name <= name \$mol_view
		title <= largeLabel @ \Maximize to screen
