$mol_number_demo $mol_row
	title @ \Number input control with various configuration
	sub /
		<= zero $mol_number
		<= one $mol_number
			value?val <=> year?val null
		<= two $mol_number
			value?val <=> year?val null
			hint \2016
		<= three $mol_number
			value?val <=> age?val 32
			hint \18-99
			enabled false
		<= four $mol_number
			value?val <=> year?val null
			string_enabled false
		<= five $mol_number
			value?val <=> age?val 32
			dec_enabled false
		<= six $mol_number
			value?val <=> year?val null
			inc_enabled false
		<= seven $mol_number
			value?val <=> year?val null
			precision_change 10
		<= eight $mol_number
			value?val <=> year?val null
			precision_view 0.01
		<= nine $mol_number
			value?val <=> year?val null
			precision 1000
